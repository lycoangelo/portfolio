{
  "extends": ["next/core-web-vitals", "plugin:sonarjs/recommended"],
  "plugins": ["unused-imports"],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "no-unused-expressions": "error",
    "no-console": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "eqeqeq": "error"
  }
}
